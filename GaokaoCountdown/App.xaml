<Application x:Class="GaokaoCountdown.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GaokaoCountdown"
             StartupUri="MainWindow.xaml" Startup="Application_Startup">
    <Application.Resources>
            <ResourceDictionary>
                <ResourceDictionary.MergedDictionaries>
                    <ResourceDictionary>
                    <FontFamily x:Key="zihun59">pack://application:,,,/Resources/字魂59号-创粗黑字体.ttf#字魂59号-创粗黑</FontFamily>
                    <FontFamily x:Key="DIN1451">pack://application:,,,/Resources/DIN 1451 LT W06 Mittelschrift.ttf#DIN1451LTW06-Mittelschrift</FontFamily>
                </ResourceDictionary>
                </ResourceDictionary.MergedDictionaries>
            </ResourceDictionary>
        </Application.Resources>
</Application>
